{% if content %}
<div {{ attributes.addClass(['buildercv_competances']) }}>
	<div class="container">
		<div class="row">
			<div class="col-md-8 competances">
				<{{settings.region_tag_title}}
				{{ region_attributes.subtitle.addClass(['title']) }}
				>
				{{ content.title }}
				</{{settings.region_tag_title}}> 
				{% if content.listes|has_value %}				
				<div {{ region_attributes.listes.removeClass(settings.region_css_listes) }}>
            	{% for item in content.listes|array_elements(region_attributes.listes.addClass('listes', settings.region_css_listes)) %}
            		{{ item }}
            	{% endfor %}
            	</div>
				{% endif %}
			</div>
			<div class="col-md-4 langues">
				<{{settings.region_tag_title2}}
				{{ region_attributes.subtitle.addClass(['title']) }}
				>
				{{ content.title2 }}
				</{{settings.region_tag_title2}}> 
				{% if content.listes2|has_value %}
				<div {{ region_attributes.listes2 }}>
            	{% for item in content.listes2|array_elements(region_attributes.listes2.addClass('listes')) %}
            		{{ item }}
            	{% endfor %}
            	</div>
				{% endif %}
			</div>
			
		</div>
	</div>
</div>
{% endif %}