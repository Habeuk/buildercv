{% if item %}
  <div class="infos field-svg d-flex">
    {% if item.value %}
      <div class="svg">
        {{ item.value }}
      </div>
    {% endif %}
    {% if item.text %}
      <div class="text font-weight-bold d-flex align-items-center">
        {{ item.text }}
      </div>
    {% endif %}
  </div>
{% endif %}
